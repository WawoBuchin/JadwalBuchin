<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buchin.jadwalbuchin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HolidayActivity"
            android:label="Holidays"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TeacherActivity"
            android:label="Teachers"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GradeActivity"
            android:label="Average: "
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ReminderActivity"
            android:label="Reminders"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".HomeWorkActivity"
            android:label="Homeworks"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TestActivity"
            android:label="Calendar"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity"></activity>
    </application>

</manifest>